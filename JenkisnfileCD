pipeline {
    agent {label 'linux'}
        stages {
            stage('kubectl cred') {
                steps {
                    withKubeConfig(caCertificate: '', clusterName: '', contextName: '', credentialsId: 'kube_config', namespace: '', serverUrl: '') {
                        sh 'kubectl get nodes'
                    }
                }
            }
        }
}
